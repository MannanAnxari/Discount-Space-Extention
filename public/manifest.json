{
  "name": "Discount Space!",
  "description": "Discount Space! Buy Anything You Want!!!",
  "version": "0.1.0",
  "manifest_version": 3,
  "action": {
    "default_title": "Discount Space!",
    "default_popup": "index.html",
    "default_icon": "logo.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "content.css"
      ]
    }
  ]
}